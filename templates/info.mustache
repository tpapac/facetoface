{{=<% %>=}}
<ion-header [translucent] = 'true'>
    <ion-toolbar color="primary">
        <ion-title>
            Informacije o sesiji
        </ion-title>
    </ion-toolbar>
</ion-header>
<ion-grid class="ion-margin">
    <%#table%>
    <%#.%>
    <ion-row>
        <ion-col>
            <%{header}%>
        </ion-col>
        <ion-col>
            <%{value}%>
        </ion-col>
    </ion-row>
    <%/.%>
    <%/table%>
</ion-grid>