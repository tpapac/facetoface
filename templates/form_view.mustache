{{=<% %>=}}
<ion-view cache="false">
<%#rows%>
<ion-header [translucent] = "true">
    <ion-toolbar color="primary">
        <ion-title>
            Informacije o sesiji
        </ion-title>
    </ion-toolbar>
</ion-header>
    <ion-grid class="ion-margin">
        <%#.%>
        <ion-row>
            <ion-col><%header%></ion-col>
            <ion-col><%{value}%></ion-col>
        </ion-row>
        <%/.%>
    </ion-grid>
<%/rows%>
</ion-view>

<script>
    $(document).ready(function() {
            location.reload();
    });
</script>
