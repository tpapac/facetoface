{{=<% %>=}}
<%#rows%>
<ion-header [translucent] = "true">
    <ion-toolbar color="primary">
        <ion-title>
            Informacije o sesiji
        </ion-title>
    </ion-toolbar>
</ion-header>
    <ion-grid class="ion-margin">
        <%#.%>
        <ion-row>
            <ion-col><%header%></ion-col>
            <ion-col><%{value}%></ion-col>
        </ion-row>
        <%/.%>
    </ion-grid>
<%/rows%>

<ion-item>
    <ion-label position="floating">Name</ion-label>
    <ion-input type="email" required class="ion-invalid"></ion-input>
    <ion-text color="danger">
        <small>Please enter your name</small>
    </ion-text>
</ion-item>


