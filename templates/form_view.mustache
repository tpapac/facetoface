<!--{{=<% %>=}}-->
{{ CONTENT_OTHERDATA.myVar }}

<table *ngFor="let item of {{ CONTENT_OTHERDATA.myVar }}">
    <ion-row *ngFor="let row of item">
        <ion-col>asd</ion-col>
    </ion-row>
</table>
<ion-grid>
    <ion-row>
        <ion-col>
            <%#tableheader%>
            <ion-row>
               <%.%>
            </ion-row>
            <%/tableheader%>
        </ion-col>
        <ion-col>
            <%#row.rows%>
                    <%.%>
            <%/row.rows%>
        </ion-col>
    </ion-row>
</ion-grid>