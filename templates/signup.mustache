{{=<% %>=}}
<ion-header [translucent] = 'true'>
    <ion-toolbar color="primary">
        <ion-title>
            Informacije o sesiji
        </ion-title>
    </ion-toolbar>
</ion-header>
    <ion-grid class="ion-margin">
        <%#table%>
        <%#.%>
            <ion-row>
                <ion-col style="border: 1px solid black">
                    <%{header}%>
                </ion-col>
                <ion-col style="border: 1px solid black">
                    <%{value}%>
                </ion-col>
            </ion-row>
        <%/.%>
        <%/table%>
    </ion-grid>



